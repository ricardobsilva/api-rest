<p id="notice"><%= notice %></p>

<h1>Listing Api V1 Shopping Lists</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_shopping_lists.each do |api_v1_shopping_list| %>
      <tr>
        <td><%= link_to 'Show', api_v1_shopping_list %></td>
        <td><%= link_to 'Edit', edit_api_v1_shopping_list_path(api_v1_shopping_list) %></td>
        <td><%= link_to 'Destroy', api_v1_shopping_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

